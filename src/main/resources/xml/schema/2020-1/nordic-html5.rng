    <define name="blocknoimggroupnotable">
        <a:documentation>common non-head elements for use with sectioning elements.</a:documentation>
        <choice>
            <ref name="p"/> <!-- p, hr, ol, ul, dl -->
            <ref name="pre"/> <!-- pre -->
            <ref name="list"/> <!-- ol, ul -->
